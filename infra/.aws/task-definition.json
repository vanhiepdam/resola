{
  "family": "resolatest",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "python:3.10-slim",
      "cpu": 0,
      "secrets": [
        {
          "name": "AWS_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:813283234169:secret:hiepdam_aws_credentials:AWS_ACCESS_KEY_ID::"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:813283234169:secret:hiepdam_aws_credentials:AWS_SECRET_ACCESS_KEY::"
        }
      ],
      "portMappings": [
        {
          "name": "backend-8000-tcp",
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "workingDirectory": "/app/",
      "entryPoint": [
        "sh",
        "-c"
      ],
      "command": [
        "/bin/sh",
        "-c",
        "/app/deployments/entrypoint.sh"
      ],
      "environment": [
        {
          "name": "DATABASE_NAME",
          "value": ""
        },
        {
          "name": "DATABASE_HOST",
          "value": ""
        },
        {
          "name": "ALLOWED_HOSTS",
          "value": ""
        },
        {
          "name": "CSRF_TRUSTED_ORIGINS",
          "value": ""
        },
        {
          "name": "DATABASE_PORT",
          "value": ""
        },
        {
          "name": "SECRET_KEY",
          "value": "3a677a%h+t_*kwr)"
        },
        {
          "name": "DEBUG",
          "value": "false"
        },
        {
          "name": "DATABASE_USER",
          "value": ""
        },
        {
          "name": "DATABASE_PASSWORD",
          "value": ""
        },
        {
          "name": "AWS_STORAGE_BUCKET_NAME",
          "value": ""
        }
      ],
      "environmentFiles": [],
      "mountPoints": [
        {
          "sourceVolume": "app",
          "containerPath": "/app/",
          "readOnly": false
        }
      ],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/resolatest",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "dependsOn": [
        {
          "containerName": "db",
          "condition": "HEALTHY"
        }
      ]
    },
    {
      "name": "db",
      "image": "postgres:14-alpine",
      "cpu": 0,
      "portMappings": [
        {
          "name": "db-5432-tcp",
          "containerPort": 5432,
          "hostPort": 5432,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "POSTGRES_USER",
          "value": ""
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": ""
        },
        {
          "name": "POSTGRES_DB",
          "value": ""
        },
        {
          "name": "PGDATA",
          "value": "/var/lib/postgresql/data/pgdata"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [
        {
          "sourceVolume": "data",
          "containerPath": "/var/lib/postgresql/data/pgdata",
          "readOnly": false
        }
      ],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/resolatest",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "sh -c 'pg_isready -U ${POSTGRES_USER}'"
        ],
        "interval": 5,
        "timeout": 5,
        "retries": 5
      }
    }
  ],
  "executionRoleArn": "arn:aws:iam::813283234169:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [
    {
      "name": "data",
      "host": {}
    },
    {
      "name": "app",
      "host": {}
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}